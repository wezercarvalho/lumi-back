{
    "name": "lumi-teste-pratico-back",
    "version": "1.0.0",
    "description": "API backend para teste pr√°tico Lumi",
    "author": "Wezer Carvalho",
    "license": "MIT",
    "main": "dist/app.js",
    "scripts": {
        "dev": "nodemon -r tsconfig-paths/register src/app.ts",
        "build": "yarn clean && tsc",
        "start": "node dist/app.js",
        "lint": "eslint \"src/**/*.{ts,js}\"",
        "lint:fix": "eslint \"src/**/*.{ts,js}\" --fix",
        "format": "prettier --write 'src/**/*.{ts,js}'",
        "test": "jest",
        "clean": "rimraf dist",
        "migration-generate": " npx sequelize-cli migration:generate --name",
        "migration-seed-generate": "npx sequelize-cli seed:generate --name",
        "migration-seed-run": "npx sequelize-cli db:seed:all",
        "migration-run": "npx sequelize-cli db:migrate"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.1",
        "module-alias": "^2.2.3",
        "pdf-parse": "^1.1.1",
        "pg": "^8.13.0",
        "reflect-metadata": "^0.2.2",
        "sequelize": "^6.37.4",
        "uuid": "^10.0.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.13",
        "@types/pdf-parse": "^1.1.4",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.1",
        "jest": "^29.7.0",
        "nodemon": "^3.1.7",
        "prettier": "^3.3.3",
        "rimraf": "^5.0.10",
        "sequelize-cli": "^6.6.2",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^4.3.5"
    },
    "_moduleAliases": {
        "@config": "src/config",
        "@controllers": "src/controllers",
        "@models": "src/models",
        "@routes": "src/routes",
        "@services": "src/services",
        "@tests": "src/tests",
        "@utils": "src/utils"
    }
}
